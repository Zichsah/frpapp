<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="phillykeyspots.frpapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="4"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="phillykeyspots.frpapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="phillykeyspots.frpapp.DashboardActivity"
            android:label="@string/title_activity_dashboard" >
        </activity>
        <activity
            android:name="phillykeyspots.frpapp.JomlFragment"
            android:label="@string/title_activity_joml_fragment" >
        </activity>
        <activity
            android:name="phillykeyspots.frpapp.ResourcesFragment"
            android:label="@string/title_activity_resources_fragment" >
        </activity>
        <activity
            android:name="phillykeyspots.frpapp.EventsFragment"
            android:label="@string/title_activity_events_fragment" >
        </activity>
    </application>

</manifest>